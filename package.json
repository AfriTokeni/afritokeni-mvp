  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --force",
    "juno:dev-start": "juno dev start",
    "juno:dev-stop": "juno dev stop",
    "juno:dev-publish": "juno config -m development",
    "juno:dev-login": "juno login --mode development",
    "build": "tsc -b && vite build",
    "build:backend": "tsc --project tsconfig.backend.json",
    "start:backend": "node dist/services/server.js",
    "dev:sms": "tsx src/services/smsServer.ts",
    "build:sms": "tsc --project tsconfig.backend.json",
    "start:sms": "node dist/services/smsServer.js",
    "lint": "eslint .",
    "preview": "vite preview",
    "format": "prettier --write .",
    "postinstall:copy-auth": "node -e \"require('fs').cpSync('node_modules/@junobuild/core/dist/workers/', './public/workers', {recursive: true});\"",
    "postinstall": "npm run postinstall:copy-auth"
  },
